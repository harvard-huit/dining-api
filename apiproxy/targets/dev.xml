<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="dev">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request/>
        <Response>
            <Step>
                <Name>am-add-cors</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response>
            <Step>
                <Name>test</Name>
            </Step>
        </Response>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties>
            <Property name="io.timeout.millis">900000</Property>
            <Property name="keepalive.timeout.millis">900000</Property>
            <Property name="connect.timeout.millis">900000</Property>
            <Property name="response.streaming.enabled">true</Property>
            <Property name="request.streaming.enabled">true</Property>
        </Properties>
        <URL>https://dining-api.dev.ats.cloud.huit.harvard.edu/ats/dining</URL>
    </HTTPTargetConnection>
</TargetEndpoint>